<?xml version="1.0" encoding="utf-8"?>
<layout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">

        <include
            android:id="@+id/titleBar"
            layout="@layout/base_head_layout" />

        <LinearLayout
            android:orientation="vertical"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/settled_data_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/underreview"/>

            <TextView
                android:id="@+id/settled_data_text1"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_18"
                android:textColor="@color/color_333333"
                android:text="提交成功，请等待审核！"/>

            <TextView
                android:id="@+id/settled_data_text2"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_12"
                android:textColor="@color/color_333333"
                android:text="预计2个工作日内审核完毕"/>
        </LinearLayout>

    </LinearLayout>
</layout>